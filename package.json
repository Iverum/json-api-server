{
  "name": "json-api-server",
  "version": "0.1.1",
  "repository": "https://github.com/Iverum/json-api-server.git",
  "description": "A NodeJS framework implementing json:api",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.9.11"
  },
  "scripts": {
    "build": "babel src -d lib",
    "start": "babel src -d lib && babel example -d build && node build/index.js",
    "test": "jest"
  },
  "author": "Blake Hair <blake.hair@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.5",
    "restify": "4.3.0",
    "restify-errors": "4.3.0",
    "restify-plugins": "1.6.0",
    "sequelize": "4.37.5",
    "sqlite3": "4.0.0",
    "yayson": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.6.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jest": "21.15.0",
    "jest": "22.4.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/example/",
      "/node_modules/"
    ]
  }
}
